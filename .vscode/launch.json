{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Storybook Debug",
      "url": "http://localhost:9080",
      "trace": true,
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/packages/<package-name>",
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/*",
        "webpack:///./*": "${webRoot}/*",
        "webpack:///src/*": "${webRoot}/*",
        "webpack:///./~/*": "${webRoot}/node_modules/*"
      }
    },
    {
      "type": "chrome",
      "name": "http://127.0.0.1:3000/packages/app-sundevil/examples/header.html",
      "request": "launch",
      "url": "http://127.0.0.1:3000/packages/app-sundevil/examples/header.html"
    }
  ]
}
